<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/linearLayout1"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@drawable/bg_mit_gitter">
	
    <RelativeLayout
        android:id="@+id/relativeLayoutWrapper"
        android:layout_centerHorizontal="true"
		android:layout_width="320dp"
		android:layout_height="wrap_content">
        
        <LinearLayout
			android:id="@+id/examResultLinearLayout1"
			android:background="@drawable/fenster_formel_navi"
			android:gravity="center"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content">
		
			<TextView
				android:id="@+android:id/text1"
				android:text="Label Label Label Label Label"
				android:textColor="@android:color/white"
				android:textSize="15sp"
				android:textStyle="bold"
				android:layout_marginTop="3dp"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content" />
		
		</LinearLayout>
		
		<ImageView
			android:src="@drawable/pfeil_rahmen_formel_navi"
			android:layout_marginTop="44dp"
			android:layout_marginRight="34dp"
			android:layout_alignParentRight="true"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content" />
			
		<ImageView
			android:id="@+id/examResultMascotImageView"
			android:src="@drawable/image_endpruefung_bestanden"
			android:layout_marginTop="16dp"
			android:layout_below="@+id/examResultLinearLayout1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
			
		<TextView
			android:text="13 Punkte"
			android:textSize="16sp"
			android:textColor="@android:color/black"
			android:textStyle="bold"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:layout_marginLeft="140dp"
			android:id="@+id/examResultPointsTextView"
			android:layout_alignTop="@+id/examResultMascotImageView"
			android:layout_marginTop="37dp" />
			
		<TextView
			android:text="35 min."
			android:textSize="16sp"
			android:textColor="@android:color/black"
			android:textStyle="bold"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:id="@+id/examResultTimeTextView"
			android:layout_below="@+id/examResultPointsTextView"
			android:layout_alignLeft="@+id/examResultPointsTextView"
			android:layout_marginTop="10dp" />
        
    </RelativeLayout>
		
	<Button
		android:id="@+id/button1"
		android:layout_height="45dp"
		android:text="@string/retry_exam"
		style="@style/CustomButton"
		android:layout_width="300dp"
		android:layout_below="@+id/relativeLayoutWrapper"
		android:layout_centerHorizontal="true"
		android:layout_marginTop="28dp"
		android:onClick="retryExam" />
		
	<Button
		android:id="@+id/button2"
		android:layout_height="45dp"
		android:text="@string/show_all_questions"
		style="@style/CustomButton"
		android:layout_width="300dp"
		android:layout_centerHorizontal="true"
		android:layout_below="@+id/button1"
		android:layout_marginTop="8dp"
		android:onClick="showAllQuestions" />
		
	<Button
		android:id="@+id/button3"
		android:layout_height="45dp"
		android:text="@string/show_all_faulty_questions"
		style="@style/CustomButton"
		android:layout_width="300dp"
		android:layout_marginTop="8dp"
		android:layout_centerHorizontal="true"
		android:layout_below="@+id/button2"
		android:layout_alignLeft="@+id/examButton"
		android:onClick="showFaultyQuestions" />
	
</RelativeLayout><!-- From: file:/D:/LindenValley/Mobicom_03062015/project/studio/fahrschulelite-android-master/fahrschuleliteandroidmaster/src/main/res/layout/exam_result.xml -->