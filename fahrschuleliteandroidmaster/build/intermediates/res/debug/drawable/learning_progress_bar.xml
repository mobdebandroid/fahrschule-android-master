<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <stroke
                android:width="1dp"
                android:color="#ffa3a3a3" />
            <gradient
				android:startColor="#ffa9a9a9"
				android:centerColor="#ffefefef"
				android:centerY="0.5"
				android:endColor="#fffcfcfc"
				android:angle="270" />
        </shape>
    </item>
    
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <stroke
	                android:width="1dp"
	                android:color="#ffd00501" />
                <gradient
					android:startColor="#fff4656c"
					android:centerColor="#ffc63235"
					android:centerY="0.5"
					android:endColor="#ff9f0608"
					android:angle="270" />
            </shape>
        </clip>
    </item>
    
    <item android:id="@android:id/progress">
        <clip>
        	<shape>
	            <corners android:radius="5dip" />
	            <stroke
	                android:width="1dp"
	                android:color="#ff05a001" />
			    <gradient
					android:startColor="#ff7bcd44"
					android:centerColor="#ff48b023"
					android:centerY="0.5"
					android:endColor="#ff1d9707"
					android:angle="270" />
			</shape>
        </clip>
    </item>
    
